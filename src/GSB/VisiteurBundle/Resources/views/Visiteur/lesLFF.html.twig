{% extends "::base.html.twig" %} 
{# empty Twig template #}
        {% block head %}
		<meta charset="utf-8"/>
	{% endblock %} 
    {% block body %}
        <body>
            <div class="text-center">
            <a href="http://127.0.0.1:8000/">
                <img src="{{ asset('img/logo_big.png') }}" alt="GSB"/> 
            </a>
            </div>
            <div class="container">
                <div class="row">
                <div class="col-md-1"></div>

                <table class="table">
                    <thead>
                        <tr>
                        <th scope="col">Mois</th>
                        <th scope="col">Quantite</th>
                        <th scope="col">Forfait</th>
                        <th scope="col">Modifier</th>

                        </tr>
                    </thead>
                    {% for value in result %}
                        <tbody>
                            <tr>
                            <td> {{ value.mois }} </td>
                            <td> {{ value.quantite }} </td>
                            <td> {{ value.fraisforfait.id }} </td>
                            <td>  <a href="/fichefrais/LigneFF/modifier/{{value.id}}" class="btn btn-primary"> Modifier</a> </td>
                            </tr>
                        </tbody>
                    {% endfor %}
                    </table>
                </div>

            </div>
        </body>
        </html>

    {% endblock %}
        
    
